                var b = $(this);\r\n                var t = b.offset().top;\r\n                //console.log(t)\r\n                if (t > 1500) {\r\n                    $(\".shelter\").fadeIn(1000);\r\n                }\r\n                else {\r\n                    $(\".shelter\").fadeOut(1000);\r\n                }\r\n\r\n            })\r\n        })\r\n    })\r\n\r\n    //回到顶部按钮\r\n    $(function () {\r\n        //当滚动条的一屏后出现\r\n        $(function () {\r\n            $(window).scroll(function () {\r\n                var dbtop = $(window).height();\r\n                if ($(window).scrollTop() > dbtop) {\r\n                    $(\"#back\").fadeIn(1000);\r\n                }\r\n                else {\r\n\r\n                    $(\"#back\").fadeOut(1000);\r\n                }\r\n            });\r\n\r\n            //当点击跳转链接后，回到页面顶部位置\r\n\r\n            $(\"#back\").click(function () {\r\n                $('body,html').animate({scrollTop: 0}, 1000);\r\n                return false;\r\n            });\r\n        });\r\n    });\r\n\r\n</script>\r\n\r\n\r\n</body>\r\n</html>\r\n",
